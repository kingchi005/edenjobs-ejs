<main style="padding: 20px">
	<strong class="text-2xl inline-block relative py-2">About Us</strong>
	<br />
	<section class="about md:flex md:gap-4">
		<img
			src="/images/pexels-andrea-piacquadio-3755440.jpg"
			alt=""
			class="w-full rounded-2xl shadow-lg mb-5 md:max-w-[500px]"
		/>
		<div class="flex flex-col justify-center">
			<h2 class="text-4xl font-bold mb-4">
				We are a leading worldwide online recruitment marketplace
			</h2>
			<p class="mb-4">
				Our vision is to help businesses get the right fit for the roles they
				want to fill and help individuals worldwide connect to the right job
				that fits their life goals. We bring benefits to competent job seekers,
				start-ups, and establishments.
			</p>
			<div class="">
				<a
					href="#"
					id="get-started"
					class="inline-block px-8 py-4 text-white bg-gradient-to-r from-blue-600 to-blue-900 rounded-lg shadow-md w-auto mx-auto items-start"
					>Get Started</a
				>
			</div>
		</div>
	</section>
	<br /><br />
	<strong class="text-2xl inline-block relative py-2">How It Works</strong>
	<section class="action text-center">
		<div
			class="main-action flex justify-center mx-auto max-w-fit gap-4 px-4 py-2 shadow-md rounded-lg bg-white"
		>
			<button id="recruiter-btn" class="active bg-blue-300 px-10">
				I am a recruiter
			</button>
			<button id="worker-btn" class="px-4 py-2 rounded cursor-pointer">
				I am looking for a job
			</button>
		</div>
		<h2 class="text-4xl font-bold my-4">Getting started is easy</h2>
		<p class="my-4">
			Create a profile and weâ€™ll match you with the best candidates that meet
			your company's standards
		</p>
		<div class="preview sm:grid sm:grid-cols-2 md:sm:grid-cols-4 sm:gap-4 my-4">
			<div class="how-card px-4 py-2 rounded bg-white">
				<img src="/images/Rectangle 11.png" alt="" class="w-full rounded" />
				<h2 class="text-4xl my-4">01</h2>
				<b>Register and Login</b>
				<p class="my-4">
					Create an account and access your saved settings on any device.
				</p>
			</div>
			<div class="how-card px-4 py-2 rounded bg-white">
				<img src="/images/Rectangle 11.png" alt="" class="w-full rounded" />
				<h2 class="text-4xl my-4">02</h2>
				<b>Fill in your company data</b>
				<p class="my-4">
					Fill in your company data and access your saved information
				</p>
			</div>
			<div class="how-card px-4 py-2 rounded bg-white">
				<img src="/images/Rectangle 11.png" alt="" class="w-full rounded" />
				<h2 class="text-4xl my-4">03</h2>
				<b>Vet applications</b>
				<p class="my-4">
					Vet applications that have been submitted and access them
				</p>
			</div>
			<div class="how-card px-4 py-2 rounded bg-white">
				<img src="/images/Rectangle 11.png" alt="" class="w-full rounded" />
				<h2 class="text-4xl my-4">04</h2>
				<b>Find your match</b>
				<p class="my-4">Find a complete match to the search you made...</p>
			</div>
		</div>
		<a
			href="#"
			id="signup"
			class="text-white inline-block px-8 py-4 bg-gradient-to-r from-blue-600 to-blue-900 rounded-lg shadow-md my-4"
			>Sign up now</a
		>
	</section>

	<br />
	<br />
	<section class="recent">
		<strong class="text-2xl inline-block relative py-4">Recent Jobs</strong>
		<br class="m-0 p-0 box-border outline-none border-none" />

		<div class="recent-jobs md:grid md:grid-cols-2 gap-10 md:gap-4 my-10 px-10">
			<!-- Repeat the recent-card div for additional job postings -->
			<% (recentJobs).map(job => { %>
			<a href="/job/<%= job.id %>" class="cursor-pointer">
				<div class="recent-card shadow-md hover:shadow-xl p-5 rounded-md">
					<div class="recent-head flex items-center mb-4">
						<img
							src="<%= job.publisher.company_logo %>"
							alt=""
							class="rounded-full w-10 h-10 object-cover inline-block mr-4"
						/>
						<div class="m-0 p-0 box-border outline-none border-none">
							<b class="name block"><%= job.title %></b>
							<small class="desc text-gray-700"
								><%= job.category.name %>
							</small>
						</div>
					</div>

					<div class="tags flex items-center">
						<div
							data-te-chip-init
							data-te-ripple-init
							class="[word-wrap: break-word] my-[5px] mr-4 flex h-[32px] cursor-pointer items-center justify-between rounded-[16px] bg-[#eceff1] px-[12px] py-0 text-[13px] font-normal normal-case leading-loose text-[#4a2363] shadow-none transition-[opacity] duration-300 ease-linear hover:!shadow-none active:bg-[#cacfd1] dark:bg-neutral-600 dark:text-neutral-200"
							data-te-close="true"
						>
							<%= job.employment_type|| "Full-time" %>
						</div>
						<div
							data-te-chip-init
							data-te-ripple-init
							class="[word-wrap: break-word] my-[5px] mr-4 flex h-[32px] cursor-pointer items-center justify-between rounded-[16px] bg-[#eceff1] px-[12px] py-0 text-[13px] font-normal normal-case leading-loose text-[#4f4f4f] shadow-none transition-[opacity] duration-300 ease-linear hover:!shadow-none active:bg-[#cacfd1] dark:bg-neutral-600 dark:text-neutral-200"
							data-te-close="true"
						>
							<%= job.is_remote?"Remote":"Onsite" %>
						</div>
						<!-- <span
							class="tag inline-block mr-4 px-4 py-2 border border-blue-500 rounded-full bg-blue-200 text-blue-500 text-sm"
							>Junior</span
						> -->
					</div>

					<p class="my-4"><%= job.description %></p>

					<div
						class="botton pt-4 border-t border-gray-200 flex items-center justify-between text-sm text-gray-700"
					>
						<div class="salary">
							Salary: N<%= job.min_salary %>.00 - N<%= job.max_salary %>.00 (<%=
							job.salary_period %>)
						</div>
						<!-- <div class="posted"></div> -->
					</div>

					<div class="items flex items-center gap-1 text-xs mt-5">
						<span class="text-xs text-gray-400"
							>Posted <%= helper.formatDate(job.published_at) %></span
						>
						<div class="text-lg text-gray-400">
							<svg
								stroke="currentColor"
								fill="currentColor"
								stroke-width="0"
								viewBox="0 0 16 16"
								height="1em"
								width="1em"
								xmlns="http://www.w3.org/2000/svg"
							>
								<path d="M8 9.5a1.5 1.5 0 1 0 0-3 1.5 1.5 0 0 0 0 3z"></path>
							</svg>
						</div>
						<span class="text-blue-400"
							><%= job._count.applications %> Applicant</span
						>
						<div class="text-lg text-gray-400">
							<svg
								stroke="currentColor"
								fill="currentColor"
								stroke-width="0"
								viewBox="0 0 16 16"
								height="1em"
								width="1em"
								xmlns="http://www.w3.org/2000/svg"
							>
								<path d="M8 9.5a1.5 1.5 0 1 0 0-3 1.5 1.5 0 0 0 0 3z"></path>
							</svg>
						</div>
						<span class="ml-1 text-red-600">
							<!--  -->
							<% const _time =
							helper.formatDate(job.expires_at).replace("ago","") %>
							<!--  -->
							<%= _time==="just now"?"Closed": "Closing in "+_time %>
						</span>
					</div>
				</div>
			</a>
			<% }) %>
		</div>
	</section>
</main>
